
import java.awt.Rectangle;

public class Tower extends Rectangle
{
	
	private static final long serialVersionUID = 1L;
	private int height=0;
	private int width=0;
	
	//Tower(X Position,Y Position,Towers Height)
	public Tower(int x, int y,int width, int height)
	{
		this.height=height;
		this.width=width;
		this.setBounds(x, y, width, height);
	}

	//returns the height of the tower
	public double getHeight()
	{
		return this.height;
	}

	//sets the height of the tower
	public void setHeight(int height)
	{
		this.height = height;
	}
	
	//returns the width of the tower
		public double getWidth()
		{
			return this.width;
		}

		//sets the width of the tower
		public void setWidth(int width)
		{
			this.width = width;
		}
	
	
	
}
